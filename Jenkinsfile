pipeline
{
  agent any
  {
    stages
    {
      stage('scm')
      {
        step
        {
          echo 'scm completed'
        }
      }
      stage('build')
      {
        step
        {
          echo 'Build completed'
        }
      }
      stage('test')
      {
        step
        {
          echo 'Testing completed'
        }
      }
      stage('docker')
      {
        step
        {
          echo 'dockerization completed'
        }
      }
    }
  }
}


      
